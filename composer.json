{
  "name": "kain/think-bit",
  "description": "基于 ThinkPHP6 设计的 RESTFul API 辅助框架",
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "topthink/framework": "^6.0.0",
    "ramsey/uuid": "^3.8",
    "danielstjules/stringy": "^3.1",
    "phpseclib/phpseclib": "^2.0",
    "lcobucci/jwt": "^3.3",
    "aliyuncs/oss-sdk-php": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "think\\bit\\": "src/"
    }
  },
  "extra": {
    "think": {
      "services": [
        "think\\bit\\service\\HashService",
        "think\\bit\\service\\CipherService",
        "think\\bit\\service\\ExtService",
        "think\\bit\\service\\JwtService",
        "think\\bit\\service\\AuthService",
        "think\\bit\\service\\OssService"
      ]
    }
  },
  "authors": [
    {
      "name": "kain",
      "email": "zhangtqx@vip.qq.com"
    }
  ],
  "support": {
    "docs": "https://think-bit.kain.net.cn",
    "email": "zhangtqx@vip.qq.com"
  },
  "license": "MIT"
}
